<div class="posts">
  {{#each postData }}
  {{!-- {{log postData}} --}}
    <div class="card">
      <div class="card-header bg-light flex-row justify-space-between">
        <h2>
          <a href="/posts/{{id}}">{{title}}</a>
        </h2>
        <div>
          Post by
          {{user.username}}
          on
          {{format_date date_created}}
        </div>
      </div>
    </div>
  {{/each}}
</div>